{
  "name": "itng",
  "version": "0.0.1",
  "description": "",
  "main": "static/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ITNG/ng-utils.git"
  },
  "author": "Ryan P Kilby <rpkilby@ncsu.edu>",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/ITNG/ng-utils/issues"
  },
  "homepage": "https://github.com/ITNG/ng-utils#readme",
  "private": true,
  "jspm": {
    "format": "esm"
  },
  "eslintConfig": {
    "extends": [
      "defaults/configurations/eslint",
      "defaults/configurations/airbnb"
    ],
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "rules": {
      "indent": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "func-names": 0,
      "curly": [
        2,
        "multi-or-nest"
      ],
      "no-reserved-keys": 0,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "varsIgnorePattern": "__"
        }
      ]
    }
  }
}
